{
  "name": "@zenbusiness/workspaces-test-b",
  "version": "1.0.0",
  "description": "Second workspace test package",
  "main": "index.js",
  "author": "Jocelyn Badgley <jocelyn.badgley@zenbusiness.com>",
  "license": "UNLICENSED",
  "scripts": {
    "style": "npm run lint-fix",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "NODE_ENV=test jest --coverage --passWithNoTests"
  },
  "engines": {
    "node": "^16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenbusiness/monorepo-sandbox.git",
    "directory": "packages/a"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "automock": false,
    "clearMocks": true,
    "collectCoverage": false,
    "resolver": "<rootDir>/../jest-resolver",
    "moduleNameMapper": {
      "^@zenbusiness/(.*)$" : "<rootDir>/../$1/"
    }
  },
  "devDependencies": {
    "@zenbusiness/eslint-config": "^6.1.0",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "jest-resolver": "^1.0.0",
    "jsdoc-to-markdown": "^7.1.1"
  },
  "dependencies": {
    "@zenbusiness/workspaces-test-a": "^1.0.0"
  }
}
